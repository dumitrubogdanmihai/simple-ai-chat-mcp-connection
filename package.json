{
  "name": "openai-chat",
  "version": "1.0.0",
  "description": "Simple AI chat with OpenAI",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mcpserver": "tsx watch src-mcp-server/McpServer.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:mcpserver\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start:mcpserver": "tsx src-mcp-server/McpServer.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "http-proxy-middleware": "^3.0.5",
    "openai": "^4.52.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "concurrently": "^9.2.1",
    "tsx": "^4.21.0",
    "typescript": "^5.4.0",
    "vite": "^5.4.0"
  }
}
